{
  "name": "smart-cart-bot",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=DEVELOPMENT webpack --progress && cross-env NODE_ENV=DEVELOPMENT node ./server",
    "lint": "eslint ./src",
    "prettier": "prettier --config ./.prettierrc.js --write \"./src/**/*.{ts,tsx}\" \"!./src/**/*.d.ts\"",
    "build": "cross-env npm run lint && cross-env NODE_ENV=PRODUCTION META_MODE=only_dts webpack --progress",
    "prepublishOnly": "cross-env META_MODE=only_dts npm run build"
  },
  "engines": {
    "node": "^18"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.21.0",
    "@babel/plugin-transform-modules-commonjs": "7.21.2",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.21.0",
    "@babel/runtime": "7.27.1",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.17",
    "@types/express-serve-static-core": "4.17.35",
    "@types/jest": "29.5.0",
    "@types/morgan": "1.9.9",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "babel-loader": "9.1.2",
    "circular-dependency-plugin": "5.2.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "4.2.1",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "prettier": "2.8.7",
    "terser-webpack-plugin": "5.3.10",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "typescript": "5.0.2",
    "webpack": "5.99.8",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.1",
    "webpack-node-externals": "3.0.0",
    "xregexp": "5.1.2"
  },
  "dependencies": {
    "axios": "1.9.0",
    "dayjs": "1.11.7",
    "debug": "4.4.0",
    "dotenv": "16.5.0",
    "eventsource": "3.0.7",
    "express": "5.1.0",
    "express-requests-logger": "4.0.0",
    "fast-json-patch": "3.1.1",
    "morgan": "1.10.0",
    "node-loader": "2.0.0",
    "react-fast-compare": "3.2.2",
    "teleform": "1.2.7",
    "telegraf": "4.16.3",
    "winston": "3.10.0"
  },
  "author": "pbelinovich",
  "license": "MIT",
  "jest": {
    "transformIgnorePatterns": [
      "^.+\\.js$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.(ts|tsx)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/dev"
    ],
    "collectCoverage": false,
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "testPathIgnorePatterns": [],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "./src/**/": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
